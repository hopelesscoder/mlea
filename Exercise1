
import glob

import numpy as np

from PIL import Image

image_list = []

for i in range(1,5):
    print(i)
    if(i==1):
        path = 'coil-100/obj1__*.png'
    if(i==2):
        path = 'coil-100/obj2__*.png'
    if(i==3):
        path = 'coil-100/obj3__*.png'
    if(i==4): path = 'coil-100/obj4__*.png'
    for filename in glob.glob(path):
        im = np.asarray(Image.open(filename)).ravel()    
        image_list.append(im)







